@inject PcfService PcfService
@using RepPortal.Models

@page "/pcflist"



@code {
	
	private string? _errorMessage;

	List<PCFHeader> pcfHeaders = new List<PCFHeader>();


	protected override async Task OnInitializedAsync()
	{
		try
		{
			var pcfFile = await PcfService.GetPCFHeadersAsync();
			if (pcfFile != null)
			{
				Console.WriteLine(pcfFile.Count);
			}
		}
		catch (Exception ex)
		{
			_errorMessage = $"Error loading PCF file: {ex.Message}";
		}
	}
}




